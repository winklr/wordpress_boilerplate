{
  "name": "wp_boilerplate-project",
  "version": "0.0.1",
  "description": "Based on Fabrica Dev Kit for Wordpress Project",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/winklr/wordpress_boilerplate.git"
  },
  "private": true,
  "author": "werk85 <moin@werk85.de> (http://www.werk85.de/)",
  "scripts": {
    "start": "gulp",
    "dc": "docker-compose",
    "sh": "docker-compose exec -u www-data wp /bin/bash",
    "shroot": "docker-compose exec wp /bin/bash",
    "wp": "docker-compose exec -u www-data -T wp wp",
    "logs": "docker-compose logs -f --tail='46' wp | sed -e \"s/ WARNING: .* into stderr: \\\"\\(NOTICE: PHP message: \\)\\{0,1\\}\\(\\[[a-zA-Z0-9 :\\-]\\{1,\\}\\]\\)\\{0,1\\} \\{0,1\\}\\(.*\\)\\\"/ ⚠️  $(tput setaf 3)\\3$(tput sgr0)/\"",
    "remove": "docker-compose down --volumes --rmi local",
    "build:production": "yarn start build --production",
    "wordmove:pull": "docker-compose run --rm wordmove pull -u -d -p -e staging",
    "wordmove:pull:production": "docker-compose run --rm wordmove pull -u -d -p -e production",
    "wordmove:push": "npm run build:production && docker-compose run --rm wordmove push -t -e staging",
    "wordmove:push:production": "npm run build:production && docker-compose run --rm wordmove push -t -e production",
    "compose": "node ./scripts/dockerCompose.js",
    "composer": "docker-compose run --rm -u www-data composer",
    "init": "node ./scripts/init.js",
    "dbbackup": "node ./scripts/dbBackup.js",
    "dbimport": "node ./scripts/dbImport.js",
    "dbimport:latest": "node ./scripts/dbImport.js -- latest",
    "dbimport:default": "node ./scripts/dbImport.js -- default"
  },
  "fabrica_dev_kit": {
    "scripts_info": {
      "run": "Run 'gulp'. Default task will watch and process source files.",
      "dc": "Run 'docker-compose', eg. 'wp_boilerplate dc ps'.",
      "sh": "Start shell on WP container.",
      "shroot": "Start shell as root on WP container.",
      "wp": "Run WP Cli. eg. 'wp_boilerplate wp option list' or 'wp_boilerplate wp -- --info'",
      "logs": "Tail WP container logs.",
      "remove": "Stop all project containers, remove their volumes and WP project image.",
      "wordmove:pull": "Pull uploads, plugins and database from remote staging server",
      "wordmove:pull:production": "Pull uploads, plugins and database from remote production server",
      "wordmove:push": "Push theme to remote staging server",
      "wordmove:push:production": "Push theme to remote production server",
      "compose": "Update docker-compose.yml with current user's id and group",
      "composer": "Run composer (PHP package manager)",
      "init": "Initialize project after cloning of repository",
      "dbbackup": "dump local db to dbDump folder",
      "dbimport:latest": "import latest db dump from dbDump folder",
      "dbimport:default": "import default db dump from dbDump folder (initialDump.sql)"
    }
  },
